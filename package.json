{
  "name": "@postanu/eslint-config",
  "version": "7.0.0",
  "description": "ESLint shareable config for Postanu",
  "author": "Eduard Aksamitov <e@euaaaio.ru>",
  "license": "MIT",
  "homepage": "https://postanu.com/",
  "repository": "postanu/eslint-config",
  "type": "module",
  "packageManager": "pnpm@10.10.0",
  "exports": {
    ".": "./index.js",
    "./ts": "./ts/index.js",
    "./vue": "./vue/index.js",
    "./astro": "./astro/index.js",
    "./next": "./src/index.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "pnpx @eslint/config-inspector --config eslint.config.js"
  },
  "dependencies": {
    "@eslint/compat": "^1.2.9",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.27.0",
    "@stylistic/eslint-plugin": "^4.4.0",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-import-x": "^4.13.3",
    "eslint-plugin-n": "^17.18.0",
    "eslint-plugin-perfectionist": "^4.13.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-unicorn": "^59.0.1",
    "globals": "^16.2.0"
  },
  "peerDependencies": {
    "astro-eslint-parser": ">=1.2.2",
    "eslint": ">=9.27.0",
    "eslint-plugin-astro": ">=1.3.1",
    "eslint-plugin-vue": ">=10.1.0",
    "typescript": ">=5.8.3",
    "typescript-eslint": ">=8.32.1"
  },
  "peerDependenciesMeta": {
    "eslint-plugin-astro": {
      "optional": true
    },
    "astro-eslint-parser": {
      "optional": true
    },
    "eslint-plugin-vue": {
      "optional": true
    },
    "typescript": {
      "optional": true
    },
    "typescript-eslint": {
      "optional": true
    }
  },
  "devDependencies": {
    "astro-eslint-parser": "^1.2.2",
    "eslint": "^9.27.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-vue": "^10.1.0",
    "jiti": "^2.4.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "vue": "^3.5.15"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowAny": [
        "eslint"
      ]
    }
  }
}
